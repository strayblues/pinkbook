{
    "manifest_version": 2,
    "name": "Pink Facebook",
    "description": "Modify Facebook CSS",
    "version": "1.0.0",
    "icons": {
        "16": "icon_16.png",
        "19": "icon_19.png",
        "32": "icon_32.png",
        "64": "icon_64.png",
        "128": "icon_128.png"
    },
    "browser_action": {
        "default_icon": "icon_19.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "css": ["pink.css"],
        "matches": ["https://www.facebook.com/*"]
    }],
    "permissions": ["activeTab"]
}